{
  "_version": "1.1.0",

"start_url": "<startUrl>",

  "sap.app": {
      "_version": "1.1.0",
      "id": "sap.ui.demo.sapfiori",
      "type": "application",
      "i18n": "i18n/i18n.properties",
      "applicationVersion": {
          "version": "1.0.0"
      },
      "title": "{{appTitle}}",
      "description": "{{appDescription}}",
      "tags": {
          "keywords": [
              "{{<keyword1>}}", "{{<keyword2>}}"
          ]
      },
      "dataSources": {
          "invoiceRemote": {
              "uri": "https://services.odata.org/V2/Northwind/Northwind.svc/",
              "type": "OData",
              "settings": {
                  "odataVersion": "2.0"
              }
          }
      }
  },
  "sap.ui": {
      "_version": "1.1.0",
      "icons": {
          "icon": "<icon>",
          "favIcon": "<favIcon>",
          "phone": "<phone>",
          "phone@2": "<phone@2>",
          "tablet": "<tablet>",
          "tablet@2": "<tablet@2>"
      },
      "deviceTypes": {
          "desktop": true,
          "tablet": true,
          "phone": true
      },
      "supportedThemes": [
          "sap_hcb",
          "sap_bluecrystal"
      ]
  },
  "sap.ui5": {
      "_version": "1.1.0",
      "resources": {
          "js": [
              {
                  "uri": "<uri>"
              }
          ],
          "css": [
              {
                  "uri": "css/style.css"
              }
          ]
      },
      "dependencies": {
          "minUI5Version": "1.60",
          "libs": {
              "sap.m": {}
          }
          
      },
      "models": {
          "i18n": {
              "type": "sap.ui.model.resource.ResourceModel",
              "settings": {
                  "bundleName": "sap.ui.demo.sapfiori.i18n.i18n",
                  "supportedLocales": [""],
                  "fallbackLocale": ""
              },
              "uri": "<uriRelativeToManifest>"
          },
          "invoice": {
              "dataSource": "invoiceRemote"
          },
          "": {
              "dataSource": "<dataSourceAlias>",
              "settings": {}
          }
      },
      "rootView": {
          "viewName": "sap.ui.demo.sapfiori.view.App",
          "type": "XML",
          "async": true,
          "id": "app"
      },
      "handleValidation": false,
      "config": {
        
      },
      "routing": {
          "config": {
              "routerClass": "sap.m.routing.Router",
              "viewType": "XML",
              "viewPath": "sap.ui.demo.sapfiori.view",
              "controlId": "app",
              "controlAggregation": "pages",
              "async": true
          },
          "routes": [
              {
                  "pattern": "",
                  "name": "overview",
                  "target": "overview"
              },
              {
                  "pattern": "detail/{invoicePath}",
                  "name": "detail",
                  "target": "detail"
              }
          ],
          "targets": {
              "overview": {
                  "viewId": "overview",
                  "viewName": "Overview"
              },
              "detail": {
                  "viewId": "detail",
                  "viewName": "Detail"
              }

          }
        
      },
      "extends": {
          "component" : "<extendedComponentId>",
    "minVersion": "<minComp1Version>",
    "extensions": {}
      },
      "contentDensities": {
          "compact": true,
          "cozy": true
      }
  },

  "sap.platform.abap": {
      "_version": "1.1.0",
      "uri": "<uri>"
  },
  "sap.platform.hcp": {
      "_version": "1.1.0",
      "uri": "<uri>"
  }
}
